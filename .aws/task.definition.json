                {
                    "name": "COGNITO_USER_CLIENT_ID",
                    "value": "5ir56aur0c0q93j43gnik2e4vl"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "pa_dev"
                },
                {
                    "name": "COGNITO_AWS_REGION",
                    "value": "us-east-1"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
